extends:
  - 'standard'
  - 'plugin:react/recommended'
plugins:
  - 'react'
settings:
  react:
    pragma: 'React'
    version: '16.5.2'
parserOptions:
  ecmaVersion: 6
  jsx: true
rules:
  space-before-function-paren:
    - error
    - never
  no-multiple-empty-lines:
    - error
    - max: 2
      maxBOF: 0
      maxEOF: 0

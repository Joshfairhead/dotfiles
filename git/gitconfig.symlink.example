# Sample gitconfig
#

[hub]
    protocol = https
[user]
    name = AUTHORNAME
    email = AUTHOREMAIL
[alias]
    br = branch
    ci = commit
    cia = commit -a
    co = checkout
    cob = checkout -b
    df = diff
    st = status -sb
    last = log -1 HEAD
    promote = !$ZSH/bin/git-promote
    wtf     = !$ZSH/bin/git-wtf
    rank-contributors = !$ZSH/bin/git-rank-contributors
    count   = !git shortlog -sn
[core]
    editor = vim
    excludesfile = ~/.gitignore
[color]
    branch = auto
    diff = auto
    status = auto
    ui = true
[difftool]
    prompt = false
[help]
    autocorrect = 1
[mergetool]
    keepBackup = false
[push]
    # See `git help config` (search for push.default)
    # for more information on different options of the below setting.
    #
    # Setting to git 2.0 default to surpress warning message
    default = simple

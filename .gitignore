# Atom sync
atom/atom.symlink/.apm
atom/atom.symlink/.node-gyp
atom/atom.symlink/blob-store
atom/atom.symlink/compile-cache
atom/atom.symlink/dev
atom/atom.symlink/nohup.out
atom/atom.symlink/packages
atom/atom.symlink/recovery
atom/atom.symlink/storage
atom/atom.symlink/package-deps-state.json
atom/atom.symlink/projects.cson

git/gitconfig.local.symlink

# duply: ignore gpg keys
duply/**/*.gpg
duply/**/*.asc

# gpg: track config only
gpg/gnupg.symlink/*
!gpg/gnupg.symlink/dirmngr.conf
!gpg/gnupg.symlink/gpg.conf
!gpg/gnupg.symlink/gpg-agent.conf

# python: track profile config only
python/ipython.symlink/**/**
!python/ipython.symlink/**/
!python/ipython.symlink/**/ipython_config.py

# ssh: track config only
ssh/ssh.symlink/*
!ssh/ssh.symlink/authorized_keys
!ssh/ssh.symlink/config
!ssh/ssh.symlink/config.git

# Ignore compiled hack
system/keyboard/disable-capslock-delay

# Ignore vim plugins
vim/vim.symlink/plugged
vim/vim.symlink/plugin

# Track weechat config/plugins only
weechat/weechat.symlink/*
!weechat/weechat.symlink/*.conf
!weechat/weechat.symlink/certs
!weechat/weechat.symlink/perl
!weechat/weechat.symlink/python
!weechat/weechat.symlink/weechat.png
